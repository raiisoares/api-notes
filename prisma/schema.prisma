generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Notes {
  id String @id @default(uuid())
  title String
  subject String
  content String
  created_at DateTime @default(now())
  status Boolean @default(false)

  @@map("notes")
}